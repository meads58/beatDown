<!DOCTYPE html>
<html lang="en" ng-app="BeatDown">
<head>
  <meta charset="UTF-8">
  <title>BeatDown</title>
  <link rel="stylesheet" href="bower_components/bootstrap/dist/css/bootstrap.css">
  <link rel="stylesheet" href="/styling/styles.css">
  <script src="bower_components/jquery/dist/jquery.js"></script>
  <script src="bower_components/angular/angular.js"></script>
  <script src="bower_components/angular-resource/angular-resource.js"></script>
  <script src="js/app.js"></script>
  <script src="js/bandImageController.js"></script>
</head>

<body ng-controller="BandImageController as bandCtrl">

  <header id="top" class="main_header">
    <nav class="navbar navbar-default">
      <h1 class="main_title">Beat Down</h1>
      <div class="bandMembers">
        <ul class="pagination">
          <li><a href="#" id="camster" href ng-mouseenter="hoverIn('camImg')" ng-mouseleave="hoverOut('camImg')">Cam</a></li>
          <li><a href="#" id="pie" href ng-mouseenter="hoverIn('pieImg')" ng-mouseleave="hoverOut('pieImg')">Pie</a></li>
          <li><a href="#" id="tooHotty" href ng-mouseenter="hoverIn('tooHotImg')" ng-mouseleave="hoverOut('tooHotImg')">TooHotty</a></li>
          <li><a href="#" id="sotham" href ng-mouseenter="hoverIn('sothamImg')" ng-mouseleave="hoverOut('sothamImg')">Sotham</a></li>
          <li><a href="#" id="ryan" href ng-mouseenter="hoverIn('ryanImg')" ng-mouseleave="hoverOut('ryanImg')">Ryan</a></li>
        </ul>
      </div>
    </nav>
  </header>



